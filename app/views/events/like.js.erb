$(".upvotes-count<%=@event.id%>").html('<%= raw escape_javascript(@event.event_upvotes.count.to_s) %>');
$(".like-unlike-link<%=@event.id%>").html('<%= raw escape_javascript(link_to (image_tag "hook-2.png"), event_unlike_path(id: @event.id), remote: true,:class=>"myhook") %>');
<% if @event.user_id!=current_user.id %>
<% if !@unread_notifications_creator.blank? %>
<% publish_to @channel1 do %>
$(".ncount").html('<%= @unread_notifications_creator.count %>')
$(".ncount").addClass('count_design');
<% end %>
<% end %>
<% end %>
<% if !@unread_notifications_organizer.blank? %>
<% if @event.organizers.first.id!=current_user.id %>
<% publish_to @channel2 do %>
$(".ncount").html('<%= @unread_notifications_organizer.count %>')
$(".ncount").addClass('count_design');

<% end %>

<% end %>
<% end %>