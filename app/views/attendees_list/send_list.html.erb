                <span style="vertical-align: baseline; color: #000000; font-style: normal; text-decoration: none; font-weight: normal; font-size: 12pt; font-family: 'Georgia';">
                  <%= link_to "@#{attendee.twitter_handle}", user_profile_url(attendee), style:"color: #13a89e; font-style: italic; font-size: 11pt; font-family: 'Georgia'; text-decoration: underline;", target:"_blank" %>
                </span>
              </p>
            </td>
          </tr>
        <% end %>
      </tbody>
